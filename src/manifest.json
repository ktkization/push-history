{
  "name": "push-history",
  "description": "A chrome extension for searching through your Pushbullet history",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "push-history",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "identity",
    "*://*.google.com/*",
    "*://*.pushbullet.com/*",
    "storage"
  ]
}
